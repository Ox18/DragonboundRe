            function ReplaceNonPrintableUnicodeChars(str, replace_with) {
                return str.replace(re_ReplaceNonPrintableUnicodeChars, replace_with)
            }

            function WriteTime(sec) {
                document.write((sec * 1000 > Date.now()) ? new Date(sec * 1000).toString() : '---')
            }

            $(function() {
                $('#profileName').html(ReplaceNonPrintableUnicodeChars($('#profileName').html(), 'à¡ '))

                //var is_confirmed
                $('#buyTokenForm').submit(function() {
                    return confirm('Are you sure to buy 1 Guild Coin at 10000 Cash? \n(You have {{{my_cash}}} Cash)')
                })

                $('#promoteSubLeader').click(function() {
                    alertify.prompt('Enter member name to convert to Semi-Leader:', function(ok, name) {
                        if (ok && name) {
                            $('#buyFormAction').val('promoteSubLeader')
                            $('#buyFormInfo').val(name)
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#demoteSubLeader').click(function() {
                    alertify.prompt('Enter Semi-Leader name to convert to member:', function(ok, name) {
                        if (ok && name) {
                            $('#buyFormAction').val('demoteSubLeader')
                            $('#buyFormInfo').val(name)
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#transferLeader').click(function() {
                    alertify.prompt('Enter Semi-Leader name to convert to Leader: (You will be changed to Semi-Leader and not be able to buy more features or control the guild anymore)', function(ok, name) {
                        if (ok && name) {
                            $('#buyFormAction').val('transferLeader')
                            $('#buyFormInfo').val(name)
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#rename').click(function() {
                    alertify.prompt('Enter new guild name:', function(ok, name) {
                        if (ok && name) {
                            $('#buyFormAction').val('rename')
                            $('#buyFormInfo').val(name)
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#plug50gp24hours').click(function() {
                    alertify.confirm('Are you sure? [+50% GP for 24 Hours]', function(ok) {
                        if (ok) {
                            $('#buyFormAction').val('plug50gp24hours')
                            $('#buyFormInfo').val('')
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#plug20gp7days').click(function() {
                    alertify.confirm('Are you sure? [+20% GP for 7 days]', function(ok) {
                        if (ok) {
                            $('#buyFormAction').val('plug50gp24hours')
                            $('#buyFormInfo').val('')
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#plug20gp30days').click(function() {
                    alertify.confirm('Are you sure? [+20% GP for 30 days]', function(ok) {
                        if (ok) {
                            $('#buyFormAction').val('plug50gp24hours')
                            $('#buyFormInfo').val('')
                            $('#buyForm').submit()
                        }
                    })
                })

                $('#plug30gold30days').click(function() {
                    alertify.confirm('Are you sure? [+30% Gold for 30 days]', function(ok) {
                        if (ok) {
                            $('#buyFormAction').val('plug50gp24hours')
                            $('#buyFormInfo').val('')
                            $('#buyForm').submit()
                        }
                    })
                })
            })
