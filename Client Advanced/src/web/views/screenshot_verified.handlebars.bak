<style>
        h1 {
            font-size: 26px;
        }

        .glow_button {
            -webkit-transition: all 0.15s ease-in-out;
            transition: all 0.15s ease-in-out;
        }
        .glow_button:hover {
            box-shadow: 0 0 10px 10px #ff0;
        }
        .glow_button:active {
            box-shadow: 0 0 10px 10px #9f9;
        }

        .hide {
            display: none;
        }

        #info {
            text-align: center;
            background: rgba(0,0,0,0.5);
        }

        #bg {
            background: black no-repeat center center fixed;
            background-size: 100% 100%;
        }

        /* Fixing Stuff */
        #game_over {
            position: relative;
            margin: auto;
        }
        #game_over,.score,#scores_lose_a,#scores_lose_b {
            background-image: url(/static/images/scores_stuff2.png);
            background-repeat:no-repeat;
        }
        .rank {
            background-image: url(/static/images/themes/classic/ranks2.png);
            background-repeat:no-repeat;
        }
        .rank21,.rank27,.rank28,.rank29,.rank30,.rank31 {
            background-image: url(/static/images/ranks2.png?2) !important
        }

    </style>
