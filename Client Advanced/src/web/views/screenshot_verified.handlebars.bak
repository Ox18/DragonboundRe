        $(function() {
            var a = JSON.parse({{{DATE}}})
            var version = a[0],
                time = a[1],
                server = a[2],
                room = a[3],
                game_mode = a[4],
                max_players = a[5],
                map = a[6],
                avatar = a[7],
                wind = a[8],
                won = a[9],
                turns = a[10],
                players = a[11],
                winners = [],
                losers = []

            var game_over = {
                won:won,
                scores:players
            }

            var TEAM_A=0, TEAM_B=1
            $('#scores_lose_a,#scores_lose_b').removeClass()
            if(game_over.won == TEAM_A || game_over.won == 'A') {
                $('#scores_lose_a').addClass('win')
                $('#scores_lose_b').removeClass()
            } else if(game_over.won == TEAM_B || game_over.won == 'B') {
                $('#scores_lose_a').removeClass()
                $('#scores_lose_b').addClass('win')
            }

            var states = ['','Computer','Left','Rank Up!'],
                state_class = ['','score_state_computer','score_state_left','score_state_rankup']

            $('.score').hide()
            $('.score_me').hide()
            for(var i=0; i<game_over.scores.length; i++) {
                var score = game_over.scores[i]
                var player_number = score[0]
                var user_id = score[1]
                var game_id = score[2]
                var rank = score[3]
                var gp_this_game = score[4]
                var gold_this_game = score[5]
                var state = score[6]
                var gp_bonus = score[7]
                var gold_bonus = score[8]
                $('#score'+player_number+' .score_name').html(game_id)
                $('#score'+player_number+' .score_rank').removeClass().addClass('score_rank rank rank'+rank)
                $('#score'+player_number+' .score_gp').html(gp_this_game+' GP')
                $('#score'+player_number+' .score_gold').html(gold_this_game+' GOLD')
                $('#score'+player_number+' .score_bonus_gp').html((gp_bonus)?'+'+gp_bonus+' GP':'')
                $('#score'+player_number+' .score_bonus_gold').html((gold_bonus)?'+'+gold_bonus+' GOLD':'')
                $('#score'+player_number+' .score_state').html(states[state]).removeClass().addClass('score_state blackShadow '+state_class[state])
                $('#score'+player_number+' .score_me').hide()
                $('#score'+player_number).show()
            }

            $('#game_over').show()

//            $('body').css({backgroundImage: 'url(/static/images/maps/'+maps['map'+map+'_bg']+')'})
            var map_bg = (MAPS[map])? MAPS[map].bg : MAPS[MAP.ADIUM].bg
            $('body').css({backgroundImage: 'url(/static/images/maps/'+map_bg+')'})

            var players_str = Math.floor(max_players/2)+'v'+Math.floor(max_players/2)
            var game_mode_str = ['Normal', 'Boss', 'Same-Same', 'Score']

            $('#server').html(server)
            $('#room').html(room)
            $('#time').html(new Date(time))
            $('#players').html(players_str)
            $('#game_mode').html(game_mode_str[game_mode])
            $('#avatars').html(avatar?'On':'Off')
            $('#wind').html(wind)
            $('#turns').html(turns)
//            $('#map').html(maps['map'+map+'_name'])
            var map_name = MAPS[map]? MAPS[map].name : 'Unknown'
            $('#map').html(map_name)
        })

